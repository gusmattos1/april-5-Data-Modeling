SELECT * FROM authors WHERE name = 'karen Melton'; # you will get id 8
SELECT * FROM articles WHERE author_id = 8;

select * from provinces where name = 'Ontario'; #you get id 14
SELECT * FROM cities WHERE province_id = 14;     # you will get 2 cities

